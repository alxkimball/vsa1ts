{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "sourceMap": true,
        "typeRoots": [
            "node_modules/@types"
        ],
        "allowJs": true /*,
        "outDir": "dist"*/
    },
    "include": [
        "wwwroot/app/**/*.ts",
        "tests/**/*.ts"
    ],
    "exclude": [
        "node_modules",
        "dist",
        "gulpfile.js",
        "karma.conf.js"
    ]
}